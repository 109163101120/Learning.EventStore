@model Learning.EventStore.Sample.Web.Models.ReadModel.Dto.InventoryItemDetails
@{
    ViewData["Title"] = "ChangeName";
}
<h2>ChangeName</h2>
@using (Html.BeginForm())
{
    <p>
        @Html.Hidden("Id", Model.Id)
        Name: @Html.TextBox("Name")<br/>
        <button name="submit">Submit</button>
        <a href="/">Back</a>
    </p>
}